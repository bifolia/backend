{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "name": "Project",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "place": {
      "type": "string",
      "required": true
    },
    "year": {
      "type": "date",
      "required": true
    },
    "cover": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true
    },
    "body": {
      "type": "dynamiczone",
      "components": [
        "layout.centered",
        "layout.full-width",
        "layout.full-width-image"
      ]
    }
  }
}
